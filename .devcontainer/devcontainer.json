{
    "name": "uORocketry",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "../"
    },
    "features": {
        "ghcr.io/devcontainers/features/common-utils:2": {
            "installZsh": false,
            "installOhMyZsh": false,
            "username": "vscode"
        }
    },
    "remoteUser": "vscode",
    "portsAttributes": {
        "3000": {
            "label": "RGS Web"
        },
        "3001": {
            "label": "RGS WS"
        },
        "3002": {
            "label": "ZeroMQ Server"
        }
    },
	"postCreateCommand": "./scripts/clean.sh",
	"customizations": {
		"vscode": {
			"extensions": [
				"svelte.svelte-vscode"
			]
		}
	}
}